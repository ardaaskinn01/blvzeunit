      "type": "object",
      "properties": {
        "referenceId": {
          "description": "Identity Number of Order, ReferenceId must be unique foreach order. ReferenceId must be uppercase.\r\nSipariş Numarası, Sipariş numarası her sipariş için benzersiz olmalıdır. Sipariş numarası büyükharflerden oluşmalıdır.",
          "type": "string"
        },
        "barcode": {
          "description": "Barcode must be same with ReferenceId. Barcode must be uppercase.\r\nBarkod ReferenceId ile aynı olmalıdır. Barkod büyükharflerden oluşmalıdır.",
          "type": "string"
        },
        "billOfLandingId": {
          "description": "Bill Of LandingId\r\nİrsaliye Numarası",
          "type": "string"
        },
