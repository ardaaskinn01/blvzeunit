          "description": "Indicates whether it is Cash On Delivery.\r\nKapıda Ödeme mi?",
          "type": "integer"
        },
        "codAmount": {
          "format": "double",
          "description": "Cash on Delivery Amount\r\nKapıda Ödeme Miktarı",
          "type": "number"
        },
        "shipmentServiceType": {
          "format": "int32",
          "description": "Shipment Service Type: 1:STANDART_TESLİMAT, 7:GUNİCİ_TESLİMAT, 8:AKŞAM_TESLİMAT\r\nGönderi Tipi",
          "type": "integer"
        },
        "packagingType": {
          "format": "int32",
          "description": "Package Type of Order:  1:DOSYA, 2:Mİ, 3:PAKET, 4:KOLİ\r\nKargo Cinsi",
          "type": "integer"
        },
        "content": {
          "description": "Content\r\nGenel İçerik Bilgisi",
          "type": "string"
        },
        "smsPreference1": {
          "format": "int32",
          "description": "SMSPreference Is Send SMS to Recipient When Shipment Reached to Destination Branch\r\nKargo varış şubesine ulaştığında alıcıya SMS gitsin mi?",
          "type": "integer"
        },
        "smsPreference2": {
          "format": "int32",
          "description": "Is Send SMS to Recipient When Shpiment First Prepare\r\nKargo ilk hazırlandığında alıcıya SMS gitsin mi?",
          "type": "integer"
        },
        "smsPreference3": {
          "format": "int32",
          "description": "Is Send SMS to Shipper When Shipment Delivered\r\nKargo teslim edildiğinde göndericiye SMS gitsin mi?",
          "type": "integer"
        },
        "paymentType": {
          "format": "int32",
          "description": "Payment Type of Order: 1:GONDERICI_ODER, 2:ALICI_ODER,3:PLATFORM_ODER. Method CreateOrder does not apply to payment type 3. \r\nÖdeme Şekli, CreateOrder metodunda 3 tipindeki ödeme şekli geçerli değildir.",
          "type": "integer"
        },
        "deliveryType": {
          "format": "int32",
          "description": "Delivery Type of Order: 1:ADRESE_TESLIM, 2:ALICISI_HABERLİ \r\nTeslim Şekli",
          "type": "integer"
        },
        "description": {
          "type": "string"
        },
        "marketPlaceShortCode": {
          "description": "Short MarketPlace Code of Order : ex: TRND, N11, GG, VIVE\r\nPazaryeri Kodu  örneğin: TRND, N11, GG, VIVE",
          "type": "string"
        },
        "marketPlaceSaleCode": {
          "description": "Sale Code of MarketPlace\r\nPazaryeri Satış Kodu",
          "type": "string"
        },
        "pudoId": {
          "description": " ",
          "type": "string"
        }
